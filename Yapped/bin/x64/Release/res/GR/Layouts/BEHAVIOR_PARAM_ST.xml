<?xml version="1.0" encoding="utf-8"?>
<layout>
  <enum name="BEHAVIOR_REF_TYPE" type="u8">
    <item value="0" name="0 - Attack" />
	<item value="1" name="1 - Bullet" />
	<item value="2" name="2 - SpEffect" />
  </enum>
  <enum name="BEHAVIOR_CATEGORY" type="u8">
    <item value="0" name="0 - No Category" />
	<item value="1" name="1 - PC - Right-Hand" />
	<item value="2" name="2 - PC - Left-hand" />
	<item value="3" name="3 - Magic" />
    <item value="4" name="4 - Magic" />
	<item value="5" name="5 - Basic" />
	<item value="6" name="6 - NPC - Right-hand" />
	<item value="7" name="7 - NPC - Left-hand" />
	<item value="9" name="9 - Kick" />
    <item value="10" name="10 - PC - Both-hand" />
    <item value="11" name="11 - PC - Both-hand" />
    <item value="12" name="12 - PC - Right-hand #2" />
  </enum>
  <enum name="STATE_BEHAVIOR_TYPE" type="u8">
    <item value="0" name="0 - None" />
	<item value="1" name="1 - Player" />
	<item value="2" name="2 - NPC" />
  </enum>
  <entry>
    <name>Behavior Variation ID</name>
    <type>s32</type>
    <default>0</default>
  </entry>
  <entry>
    <name>Behavior Judge ID</name>
    <type>s32</type>
    <default>0</default>
  </entry>
  <entry>
    <name>State Behavior Type</name>
    <type>u8</type>
    <default>0</default>
    <enum>STATE_BEHAVIOR_TYPE</enum>
  </entry>
  <entry>
    <name>Reference Type</name>
    <type>u8</type>
    <default>0</default>
    <enum>BEHAVIOR_REF_TYPE</enum>
  </entry>
  
  <entry>
    <name>padding</name>
    <type>dummy8</type>
    <size>2</size>
  </entry>
  <entry>
    <name>Reference ID</name>
    <type>s32</type>
    <default>0</default>
  </entry>
  <entry>
    <name>Poise Cost</name>
    <type>f32</type>
    <default>0</default>
  </entry>
  <entry>
    <name>Stamina Cost</name>
    <type>s32</type>
    <default>0</default>
  </entry>
  <entry>
    <name>Durability Cost</name>
    <type>s32</type>
    <default>0</default>
  </entry>
  <entry>
    <name>Category</name>
    <type>u8</type>
    <default>0</default>
    <description>Determines compatibility with special effects that affect certain types of attacks.</description>
    <enum>BEHAVIOR_CATEGORY</enum>
  </entry>
  <entry>
    <name>Humanity Cost</name>
    <type>u8</type>
    <default>0</default>
  </entry>
  <!-- Verified zero -->
  <entry>
    <name>padding</name>
    <type>dummy8</type>
    <size>2</size>
  </entry>
</layout>